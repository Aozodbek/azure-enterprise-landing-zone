{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "eastus2"
    },
    "environment": {
      "value": "prod"
    },
    "resourceGroupName": {
      "value": "rg-terraform-state"
    },
    "stateContainers": {
      "value": ["tfstate"]
    },
    "allowedIpRanges": {
      "value": [
        "203.0.113.0/24"
      ]
    },
    "allowedVnetSubnetIds": {
      "value": [
        "/subscriptions/<subscription-id>/resourceGroups/rg-networking-prod-eastus2/providers/Microsoft.Network/virtualNetworks/vnet-hub-prod-eastus2/subnets/ManagementSubnet"
      ]
    },
    "pipelineIdentityObjectId": {
      "value": "<service-principal-object-id>"
    },
    "enableDefenderForStorage": {
      "value": true
    },
    "enableResourceLock": {
      "value": true
    },
    "logAnalyticsWorkspaceId": {
      "value": "/subscriptions/<subscription-id>/resourceGroups/rg-monitoring-prod-eastus2/providers/Microsoft.OperationalInsights/workspaces/<workspace-name>"
    },
    "blobSoftDeleteRetentionDays": {
      "value": 90
    },
    "containerSoftDeleteRetentionDays": {
      "value": 30
    },
    "tags": {
      "value": {
        "environment":         "prod",
        "cost-center":         "infrastructure",
        "owner":               "platform-team",
        "data-classification": "restricted",
        "managed-by":          "arm-bootstrap",
        "project":             "azure-enterprise-landing-zone"
      }
    }
  }
}
